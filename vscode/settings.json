{
    "[django-html]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnSave": true,

    },
    "python.sortImports.args": ["--atomic", "--profile", "black"],
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "asciidoc.wkhtmltopdf_path": "/usr/bin/wkhtmltopdf",
    "autoDocstring.docstringFormat": "sphinx",
    "autoDocstring.startOnNewLine": true,
    "editor.fontFamily": "'FiraCode Nerd Font'",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.rulers": [
        80,
        100
    ],
    "editor.mouseWheelZoom": true,
    "explorer.confirmDelete": false,
    "files.eol": "\n",
    "go.toolsManagement.autoUpdate": true,
    "jupyter.sendSelectionToInteractiveWindow": true,
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "tabnine.experimentalAutoImports": true,
    "terminal.integrated.fontFamily": "'SauceCodePro Nerd Font'",
    "terminal.integrated.inheritEnv": false,
    "workbench.colorTheme": "Dracula Soft",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.iconTheme": "vscode-icons",
    "material-icon-theme.showWelcomeMessage": false,
    "editor.quickSuggestionsDelay": 100,
    "terminal.integrated.fontSize": 18,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "terminal.integrated.lineHeight": 1.3,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "redhat.telemetry.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.defaultInterpreterPath": "/home/fbidu/.pyenv/versions/3.8.12/bin/python",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false,
        "ini": true,
        "python": true,
        "shellscript": true
    },
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "liveshare.presence": true,
    "python.analysis.typeCheckingMode": "basic"
}
